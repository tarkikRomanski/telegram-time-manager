{
  "name": "timemanagerbott",
  "version": "0.0.5",
  "description": "Time manager bot for the Telegram",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js --exec babel-node",
    "build": "babel . -d dist --presets es2015,stage-2 --ignore spec.js,node_modules",
    "lint": "eslint .",
    "start-dev": "npm run build && npm run start"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mongoose": "^5.4.19",
    "node-telegram-bot-api": "^0.30.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-cli": "6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-inline-import": "^2.0.1",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "dotenv": "^7.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
